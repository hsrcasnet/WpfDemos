<Application x:Uid="Application_1" x:Class="WpfLocalizationResx.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:wpfcontrols="http://www.west-wind.com/WpfControls"             
    StartupUri="StartForm.xaml">
    <Application.Resources>
        <LinearGradientBrush x:Uid="LinearGradientBrush_4" x:Key="WindowBackgroundBrush" EndPoint=".75,.75" StartPoint="0,0">
            <GradientStop x:Uid="GradientStop_7" Color="#FFF3F4F7" Offset="0.991"/>
            <GradientStop x:Uid="GradientStop_8" Color="#FFB6B6B6" Offset="0"/>
        </LinearGradientBrush>


        <LinearGradientBrush x:Uid="LinearGradientBrush_2" x:Key="ShineBrush" EndPoint=".75,.75" StartPoint="0,0">
            <GradientStop x:Uid="GradientStop_3" Color="#FF5B5B5B" Offset="0.991"/>
            <GradientStop x:Uid="GradientStop_4" Color="#FF222222" Offset="0"/>
        </LinearGradientBrush>
        <LinearGradientBrush x:Uid="LinearGradientBrush_3" x:Key="ShineColor" EndPoint="0,1" StartPoint="0,0">
            <GradientStop x:Uid="GradientStop_5" Color="#FFFFFFCC" Offset="1"/>
            <GradientStop x:Uid="GradientStop_6" Color="#FFFFFFFF" Offset="0"/>
        </LinearGradientBrush>

        <!--<ObjectDataProvider x:Key="ResExtensionManager" ObjectInstance="{x:Type wpfcontrols:ResExtensionManager}" />-->
    </Application.Resources>
</Application>
