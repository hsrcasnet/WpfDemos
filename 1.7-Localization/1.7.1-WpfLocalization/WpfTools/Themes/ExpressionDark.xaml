    <SolidColorBrush x:Uid="SolidColorBrush_6" x:Key="HoverBrush" Color="{StaticResource HoverColor}" />
    <SolidColorBrush x:Uid="SolidColorBrush_7" x:Key="GlyphBrush" Color="#FFD1D1D1" />

    <LinearGradientBrush x:Uid="LinearGradientBrush_1" x:Key="PressedBrush" EndPoint="0.5,0.971" StartPoint="0.5,0.042">
        <GradientStop x:Uid="GradientStop_1" Color="#4C000000" Offset="0" />
        <GradientStop x:Uid="GradientStop_2" Color="#26FFFFFF" Offset="1" />
        <GradientStop x:Uid="GradientStop_3" Color="#4C000000" Offset="0.467" />
        <GradientStop x:Uid="GradientStop_4" Color="#26FFFFFF" Offset="0.479" />
    </LinearGradientBrush>

    <SolidColorBrush x:Uid="SolidColorBrush_8" x:Key="ControlBackgroundBrush" Color="#FF333333" />

    <LinearGradientBrush x:Uid="LinearGradientBrush_2" x:Key="SelectedBackgroundBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop x:Uid="GradientStop_5" Color="{StaticResource TopGradientSelectColor}" Offset="0" />
        <GradientStop x:Uid="GradientStop_6" Color="{StaticResource BottomGradientSelectColor}" Offset="1" />
