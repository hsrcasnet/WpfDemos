    	<GradientStop x:Uid="GradientStop_8" Color="#FF333333" Offset="1"/>
    </LinearGradientBrush>

    <!-- EXPRESSION COMMON VALUES -->

    <LinearGradientBrush x:Uid="LinearGradientBrush_4" x:Key="ShineBrush" EndPoint="0.5,0.971" StartPoint="0.5,0.042">
