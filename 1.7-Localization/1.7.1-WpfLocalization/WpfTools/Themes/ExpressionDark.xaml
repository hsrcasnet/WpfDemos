    <!-- Main Colors -->

    <Color x:Uid="Color_1" x:Key="MainColor">#FF595959</Color>
    <Color x:Uid="Color_2" x:Key="HoverColor">#FF393939</Color>
    <Color x:Uid="Color_3" x:Key="DefaultColor">#FF9BB1C5</Color>
    <SolidColorBrush x:Uid="SolidColorBrush_1" x:Key="LightBrush" Color="#FF333333" />
    <SolidColorBrush x:Uid="SolidColorBrush_2" x:Key="ShadeBrush" Color="#FF434343" />

    <Color x:Uid="Color_4" x:Key="TopGradientSelectColor">#FFFFFFFF</Color>
    <Color x:Uid="Color_5" x:Key="BottomGradientSelectColor">#FF737373</Color>

    <Color x:Uid="Color_6" x:Key="BlackColor">#FF000000</Color>
    <Color x:Uid="Color_7" x:Key="WhiteColor">#FFFFFFFF</Color>

    <SolidColorBrush x:Uid="SolidColorBrush_3" x:Key="TextBrush" Color="#FFFFFFFF" />
