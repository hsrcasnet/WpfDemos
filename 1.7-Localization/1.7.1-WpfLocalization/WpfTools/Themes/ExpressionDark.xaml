            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Uid="Style_5" TargetType="{x:Type CheckBox}">
        <Setter x:Uid="Setter_13" Property="SnapsToDevicePixels" Value="true" />
        <Setter x:Uid="Setter_14" Property="FocusVisualStyle" Value="{DynamicResource CheckBoxFocusVisual}" />
        <Setter x:Uid="Setter_15" Property="Background" Value="{DynamicResource NormalBrush}" />
        <Setter x:Uid="Setter_16" Property="Foreground" Value="{DynamicResource TextBrush}" />
        <Setter x:Uid="Setter_17" Property="BorderBrush" Value="{DynamicResource NormalBorderBrush}" />
        <Setter x:Uid="Setter_18" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_5" TargetType="{x:Type CheckBox}">
                    <ControlTemplate.Resources>
                        <Storyboard x:Uid="Storyboard_7" x:Key="HoverOn">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_9" BeginTime="00:00:00" Storyboard.TargetName="BackgroundOverlay" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_9" KeyTime="00:00:00.1000000" Value="1" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_8" x:Key="HoverOff">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_10" BeginTime="00:00:00" Storyboard.TargetName="BackgroundOverlay" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_10" KeyTime="00:00:00.4000000" Value="0" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_9" x:Key="PressedOn">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_11" BeginTime="00:00:00" Storyboard.TargetName="PressedRectangle" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_11" KeyTime="00:00:00.1000000" Value="1" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_10" x:Key="PressedOff">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_12" BeginTime="00:00:00" Storyboard.TargetName="PressedRectangle" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_12" KeyTime="00:00:00.3000000" Value="0" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_11" x:Key="CheckedTrue">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_13" Storyboard.TargetName="CheckIcon" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_13" KeyTime="00:00:00.1000000" Value="1" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_12" x:Key="CheckedFalse">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_14" Storyboard.TargetName="CheckIcon" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_14" KeyTime="00:00:00.4000000" Value="0" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_13" x:Key="IndeterminateOn">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_15" Storyboard.TargetName="IndeterminateIcon" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_15" KeyTime="00:00:00.2000000" Value="1" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_14" x:Key="IndeterminateOff">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_16" Storyboard.TargetName="IndeterminateIcon" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_16" KeyTime="00:00:00.4000000" Value="0" />
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_15" x:Key="FocussedOn">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_17" BeginTime="00:00:00" Storyboard.TargetName="ContentFocusVisualElement" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_17" KeyTime="00:00:00.1000000" Value="1"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_16" x:Key="FocussedOff">
                            <DoubleAnimationUsingKeyFrames x:Uid="DoubleAnimationUsingKeyFrames_18" BeginTime="00:00:00" Storyboard.TargetName="ContentFocusVisualElement" Storyboard.TargetProperty="(UIElement.Opacity)">
                                <SplineDoubleKeyFrame x:Uid="SplineDoubleKeyFrame_18" KeyTime="00:00:00.3000000" Value="0"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </ControlTemplate.Resources>
                    <BulletDecorator x:Uid="bulletDecorator" Background="Transparent" x:Name="bulletDecorator">
                        <BulletDecorator.Bullet>
                            <Grid x:Uid="Grid_1" Width="16" Height="16">
                                <Rectangle x:Uid="Rectangle_4" Height="14" Margin="1" x:Name="Background" Width="14" Fill="{TemplateBinding Background}" Stroke="{TemplateBinding BorderBrush}" StrokeThickness="{TemplateBinding BorderThickness}" />
                                <Rectangle x:Uid="BackgroundOverlay" Height="14" Margin="1" x:Name="BackgroundOverlay" Width="14" Opacity="0" Fill="{x:Null}" Stroke="{StaticResource HoverBrush}" StrokeThickness="2" />
                                <Rectangle x:Uid="PressedRectangle" Height="16" x:Name="PressedRectangle" Width="16" IsHitTestVisible="false" Opacity="0" Stroke="{StaticResource HoverBrush}" StrokeThickness="1" />
                                <Path x:Uid="CheckIcon" Height="10" Margin="1,1,0,1.5" x:Name="CheckIcon" Width="10.5" Opacity="0" Fill="{StaticResource GlyphBrush}" Stretch="Fill" Data="M102.03442,598.79645 L105.22962,597.78918 L106.78825,600.42358 C106.78825,600.42358 108.51028,595.74304 110.21724,593.60419 C112.00967,591.35822 114.89314,591.42316 114.89314,591.42316 C114.89314,591.42316 112.67844,593.42645 111.93174,594.44464 C110.7449,596.06293 107.15683,604.13837 107.15683,604.13837 z" />
                                <Rectangle x:Uid="IndeterminateIcon" Height="8" x:Name="IndeterminateIcon" Width="10" Opacity="0" Fill="{StaticResource GlyphBrush}" />
                                <Rectangle x:Uid="DisabledVisualElement" Height="14" x:Name="DisabledVisualElement" Width="14" Opacity="0" Fill="{StaticResource DisabledBackgroundBrush}" />
                                <Rectangle x:Uid="ContentFocusVisualElement" Height="16" x:Name="ContentFocusVisualElement" Width="16" IsHitTestVisible="false" Opacity="0" Stroke="{StaticResource HoverShineBrush}" StrokeThickness="1" />
                                <Rectangle x:Uid="ShineRectangle" Height="12" x:Name="ShineRectangle" Width="14" IsHitTestVisible="false" Opacity="1" Stroke="{x:Null}" StrokeThickness="1" Fill="{StaticResource ShineBrush}" Margin="2,2,2,2" />
                            </Grid>
                        </BulletDecorator.Bullet>
                        <ContentPresenter x:Uid="ContentPresenter_2" RecognizesAccessKey="True" Margin="5,0,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" />
                    </BulletDecorator>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_7" Property="IsFocused" Value="True">
                            <Trigger.ExitActions>
                                <BeginStoryboard x:Uid="BeginStoryboard_4" Storyboard="{StaticResource FocussedOff}" x:Name="FocussedOff_BeginStoryboard"/>
                            </Trigger.ExitActions>
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Uid="FocussedOn_BeginStoryboard" Storyboard="{StaticResource FocussedOn}" x:Name="FocussedOn_BeginStoryboard"/>
                            </Trigger.EnterActions>
                        </Trigger>
                        <MultiTrigger x:Uid="MultiTrigger_1">
                            <MultiTrigger.Conditions>
                                <Condition x:Uid="Condition_1" Property="IsChecked" Value="True" />
                                <Condition x:Uid="Condition_2" Property="IsThreeState" Value="True" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.EnterActions>
                                <BeginStoryboard x:Uid="ThreeStateOn_BeginStoryboard" x:Name="ThreeStateOn_BeginStoryboard" Storyboard="{StaticResource CheckedFalse}" />
                            </MultiTrigger.EnterActions>
                            <MultiTrigger.ExitActions>
                                <BeginStoryboard x:Uid="ThreeStateOff_BeginStoryboard" x:Name="ThreeStateOff_BeginStoryboard" Storyboard="{StaticResource CheckedTrue}" />
                            </MultiTrigger.ExitActions>
                        </MultiTrigger>
                        <MultiTrigger x:Uid="MultiTrigger_2">
                            <MultiTrigger.Conditions>
                                <Condition x:Uid="Condition_3" Property="IsChecked" Value="{x:Null}" />
                                <Condition x:Uid="Condition_4" Property="IsThreeState" Value="True" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.EnterActions>
                                <BeginStoryboard x:Uid="ThreeStateOn1_BeginStoryboard" x:Name="ThreeStateOn1_BeginStoryboard" Storyboard="{StaticResource IndeterminateOn}" />
                            </MultiTrigger.EnterActions>
                            <MultiTrigger.ExitActions>
                                <BeginStoryboard x:Uid="ThreeStateOff1_BeginStoryboard" x:Name="ThreeStateOff1_BeginStoryboard" Storyboard="{StaticResource IndeterminateOff}" />
                            </MultiTrigger.ExitActions>
                            <Setter x:Uid="Setter_19" Property="Visibility" TargetName="CheckIcon" Value="Hidden"/>
                        </MultiTrigger>
                        <Trigger x:Uid="Trigger_8" Property="IsChecked" Value="false">
                            <Trigger.ExitActions>
                                <BeginStoryboard x:Uid="CheckedTrue_BeginStoryboard" Storyboard="{StaticResource CheckedTrue}" x:Name="CheckedTrue_BeginStoryboard"/>
                            </Trigger.ExitActions>
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Uid="BeginStoryboard_5" Storyboard="{StaticResource CheckedFalse}"/>
                            </Trigger.EnterActions>
                        </Trigger>
                        <Trigger x:Uid="Trigger_9" Property="IsMouseOver" Value="true">
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Uid="BeginStoryboard_6" Storyboard="{StaticResource HoverOn}"/>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard x:Uid="BeginStoryboard_7" Storyboard="{StaticResource HoverOff}"/>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger x:Uid="Trigger_10" Property="IsEnabled" Value="false">
                            <Setter x:Uid="Setter_20" Property="Fill" Value="{DynamicResource DisabledBackgroundBrush}" TargetName="Background" />
                            <Setter x:Uid="Setter_21" Property="Stroke" Value="{DynamicResource DisabledBorderBrush}" TargetName="Background" />
                            <Setter x:Uid="Setter_22" Property="Foreground" Value="{DynamicResource DisabledForegroundBrush}" />
                            <Setter x:Uid="Setter_23" Property="Opacity" TargetName="bulletDecorator" Value="0.5"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_6" TargetType="{x:Type RadioButton}">
        <Setter x:Uid="Setter_24" Property="SnapsToDevicePixels" Value="true" />
