    <SolidColorBrush x:Uid="SolidColorBrush_9" x:Key="DisabledForegroundBrush" Color="#888" />
    <SolidColorBrush x:Uid="SolidColorBrush_10" x:Key="DisabledBackgroundBrush" Color="#FF787878" />
    <SolidColorBrush x:Uid="SolidColorBrush_11" x:Key="DisabledBorderBrush" Color="#AAA" />    
    <LinearGradientBrush x:Uid="LinearGradientBrush_3" x:Key="WindowBackgroundBrush" EndPoint="0.5,1" StartPoint="0.5,0" MappingMode="RelativeToBoundingBox" >
