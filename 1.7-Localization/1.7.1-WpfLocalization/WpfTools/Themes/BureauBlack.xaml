    <Style x:Uid="Style_2" TargetType="{x:Type Button}" BasedOn="{x:Null}">
        <Setter x:Uid="Setter_2" Property="FocusVisualStyle" Value="{DynamicResource NuclearButtonFocusVisual}" />
        <Setter x:Uid="Setter_3" Property="Foreground" Value="#FF042271" />
        <Setter x:Uid="Setter_4" Property="FontFamily" Value="Trebuchet MS" />
        <Setter x:Uid="Setter_5" Property="FontSize" Value="12" />
        <Setter x:Uid="Setter_6" Property="Padding" Value="3" />
        <Setter x:Uid="Setter_7" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_2" TargetType="{x:Type Button}">
                    <ControlTemplate.Resources>
                        <Storyboard x:Uid="Storyboard_1" x:Key="HoverOn">
                            <DoubleAnimation x:Uid="DoubleAnimation_1" Duration="00:00:00.1000000" Storyboard.TargetName="BackgroundOver" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation x:Uid="DoubleAnimation_2" Duration="00:00:00.1000000" Storyboard.TargetName="BackgroundOver_Highlight" Storyboard.TargetProperty="Opacity" To="0.65" />
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_2" x:Key="HoverOff">
                            <DoubleAnimation x:Uid="DoubleAnimation_3" Duration="00:00:00.4000000" Storyboard.TargetName="BackgroundOver" Storyboard.TargetProperty="Opacity" To="0" />
                            <DoubleAnimation x:Uid="DoubleAnimation_4" Duration="00:00:00.4000000" Storyboard.TargetName="BackgroundOver_Highlight" Storyboard.TargetProperty="Opacity" To="0" />
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_3" x:Key="PressedOn">
                            <DoubleAnimation x:Uid="DoubleAnimation_5" Duration="00:00:00.1000000" Storyboard.TargetName="BackgroundPressed" Storyboard.TargetProperty="Opacity" To="0.84" />
                            <DoubleAnimation x:Uid="DoubleAnimation_6" Duration="00:00:00.1000000" Storyboard.TargetName="BackgoundPressed_Highlight" Storyboard.TargetProperty="Opacity" To="0.65" />
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_4" x:Key="PressedOff">
                            <DoubleAnimation x:Uid="DoubleAnimation_7" Duration="00:00:00.4000000" Storyboard.TargetName="BackgroundPressed" Storyboard.TargetProperty="Opacity" To="0" />
                            <DoubleAnimation x:Uid="DoubleAnimation_8" Duration="00:00:00.4000000" Storyboard.TargetName="BackgoundPressed_Highlight" Storyboard.TargetProperty="Opacity" To="0" />
                        </Storyboard>
                        <Storyboard x:Uid="Storyboard_5" x:Key="DisabledOn">
                            <ObjectAnimationUsingKeyFrames x:Uid="ObjectAnimationUsingKeyFrames_1" BeginTime="00:00:00" Storyboard.TargetName="DisabledVisualElement" Storyboard.TargetProperty="(UIElement.Visibility)">
                                <DiscreteObjectKeyFrame x:Uid="DiscreteObjectKeyFrame_1" KeyTime="00:00:00.1000000" Value="{x:Static Visibility.Visible}" />
